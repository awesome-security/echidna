#!/usr/bin/perl -w

use lib '../lib';
use strict;
use v5.10;

use NSMF::Node::CXTRACKER;
use Data::Dumper;

# Creating new CXTRACKER Node
my $cxt = NSMF::Node::CXTRACKER->new;

# Loading configuration variables
$cxt->load_config('/etc/nsmf-node/cxtracker.conf');

# Connect and Authenticate
$cxt->sync;

#  Same as above
#
#  $cxt->connect;
#  $cxt->authenticate;

$cxt->run;

