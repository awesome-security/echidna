DEPENDENCIES:

  - Perl POE
  - Perl ObjectDriver
  - Perl YAML::Tiny
  - Perl Module::Pluggable
  - Perl JSON


UBUNTU INSTALL:
  # sudo apt-get install libpoe-perl libdbd-mysql-perl libyaml-tiny-perl libjson-perl


FEDORA INSTALL
  # sudo yum install perl-POE perl-Data-ObjectDriver perl-YAML-Tiny perl-TimeDate perl-JSON

